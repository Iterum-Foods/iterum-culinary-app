{
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": [
    {
      "site": "iterum-culinary-app",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "!**/.nojekyll",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/",
          "destination": "/index.html"
        },
        {
          "source": "/app/**",
          "destination": "/index.html"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "iterum-culinary-app2",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "!**/.nojekyll",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/",
          "destination": "/landing.html"
        },
        {
          "source": "/tech-stage",
          "destination": "/landing.html"
        },
        {
          "source": "/business-plan",
          "destination": "/landing.html"
        },
        {
          "source": "/investors",
          "destination": "/landing.html"
        },
        {
          "source": "**",
          "destination": "/landing.html"
        }
      ]
    }
  ]
}
